{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    },
    {
      "src": "*.html",
      "use": "@vercel/static"
    },
    {
      "src": "*.json",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/partides/(.*)",
      "dest": "/server.js"
    },
    {
      "src": "/chuecos.html",
      "dest": "/chuecos.html"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}
